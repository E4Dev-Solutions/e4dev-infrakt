# Generated by infrakt â€” {{ db_type }} database: {{ name }}
services:
  {{ name }}:
    image: {{ image }}
    container_name: infrakt-db-{{ name }}
    restart: unless-stopped
{% if env_vars %}
    environment:
{% for key, value in env_vars.items() %}
      {{ key }}: "{{ value }}"
{% endfor %}
{% endif %}
    volumes:
      - {{ volume }}
    ports:
      - "127.0.0.1:{{ port }}:{{ port }}"
    networks:
      - infrakt

volumes:
  {{ name }}_data:

networks:
  infrakt:
    name: infrakt
    external: true
